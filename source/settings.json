{
  "name": "Lunch Break",
  "version": "1.10.4",
  "images": [
    {
      "variable": "header_logo",
      "label": "Header image",
      "description": "Good for an image up to 140 pixels high"
    }
  ],
  "image_sets": [
    {
      "variable": "slideshow_images",
      "label": "Homepage slideshow",
      "description": "1000px by 400px images recommended"
    }
  ],
  "preset_styles": {
    "preview": {
      "title_font": "primary_font",
      "body_font": "secondary_font",
      "text_color": "text_color",
      "background_color": "background_color"
    },
    "presets": [
      {
        "group_name": "Classic",
        "styles": [
          {
            "style_name": "Classic #1",
            "fonts": {
              "primary_font": "Open Sans",
              "secondary_font": "Open Sans"
            },
            "colors": {
              "background_color": "#FFFFFF",
              "header_background_color": "#FAFAFA",
              "header_text_color": "#135884",
              "text_color": "#111111",
              "secondary_text_color": "#404147",
              "link_hover_color": "#135884",
              "button_text_color": "#FFFFFF",
              "button_background_color": "#135884",
              "button_background_hover_color": "#193648",
              "announcement_text_color": "#FFFFFF",
              "announcement_background_color": "#135884"
            }
          },
          {
            "style_name": "Classic #2",
            "fonts": {
              "primary_font": "DM Sans",
              "secondary_font": "DM Sans"
            },
            "colors": {
              "background_color": "#141415",
              "header_background_color": "#141415",
              "header_text_color": "#F8F8F8",
              "text_color": "#F8F8F8",
              "secondary_text_color": "#C6C6C6",
              "link_hover_color": "#F8F8F8",
              "button_text_color": "#141415",
              "button_background_color": "#F8F8F8",
              "button_background_hover_color": "#C6C6C6",
              "announcement_text_color": "#141415",
              "announcement_background_color": "#F8F8F8"
            }
          }
        ]
      },
      {
        "group_name": "Elegant",
        "styles": [
          {
            "style_name": "Elegant #1",
            "fonts": {
              "primary_font": "Prata",
              "secondary_font": "Karla"
            },
            "colors": {
              "background_color": "#F6F4F4",
              "header_background_color": "#EDE8E8",
              "header_text_color": "#0F1B27",
              "text_color": "#0F1B27",
              "secondary_text_color": "#0F1B27",
              "link_hover_color": "#8A5A5A",
              "button_text_color": "#0F1B27",
              "button_background_color": "#DBD1D1",
              "button_background_hover_color": "#C7B3B3",
              "announcement_text_color": "#F1F1F1",
              "announcement_background_color": "#8A5A5A"
            }
          }
        ]
      },
      {
        "group_name": "Playful",
        "styles": [
          {
            "style_name": "Playful #1",
            "fonts": {
              "primary_font": "Syne",
              "secondary_font": "Syne"
            },
            "colors": {
              "background_color": "#FFFFFF",
              "header_background_color": "#FFFFFF",
              "header_text_color": "#0000FF",
              "text_color": "#0000FF",
              "secondary_text_color": "#0000FF",
              "link_hover_color": "#000098",
              "button_text_color": "#0000FF",
              "button_background_color": "#FFE500",
              "button_background_hover_color": "#EBD300",
              "announcement_text_color": "#0000FF",
              "announcement_background_color": "#FFE500"
            }
          },
          {
            "style_name": "Playful #2",
            "fonts": {
              "primary_font": "Quicksand",
              "secondary_font": "Quicksand"
            },
            "colors": {
              "background_color": "#FFE9F4",
              "header_background_color": "#FFFBFD",
              "header_text_color": "#243588",
              "text_color": "#243588",
              "secondary_text_color": "#243588",
              "link_hover_color": "#0C143B",
              "button_text_color": "#243588",
              "button_background_color": "#FFFA8D",
              "button_background_hover_color": "#FFF520",
              "announcement_text_color": "#243588",
              "announcement_background_color": "#FFFA8D"
            }
          }
        ]
      }
    ]
  },
  "fonts": [
    {
      "variable": "primary_font",
      "label": "Header Font",
      "default": "Open Sans"
    },
    {
      "variable": "secondary_font",
      "label": "Secondary Font",
      "default": "Open Sans"
    }
  ],
  "colors": [
    {
      "variable": "background_color",
      "label": "Background",
      "default": "#FFFFFF"
    },
    {
      "variable": "header_background_color",
      "label": "Header Background",
      "default": "#FAFAFA"
    },
    {
      "variable": "header_text_color",
      "label": "Header Text",
      "default": "#135884"
    },
    {
      "variable": "text_color",
      "label": "Primary Text",
      "default": "#111111"
    },
    {
      "variable": "secondary_text_color",
      "label": "Secondary Text",
      "default": "#404147"
    },
    {
      "variable": "link_hover_color",
      "label": "Link Hover",
      "default": "#135884"
    },
    {
      "variable": "button_text_color",
      "label": "Button Text",
      "default": "#FFFFFF"
    },
    {
      "variable": "button_background_color",
      "label": "Button Background color",
      "default": "#135884"
    },
    {
      "variable": "button_background_hover_color",
      "label": "Button Background Hover",
      "default": "#193648"
    },
    {
      "variable": "announcement_text_color",
      "label": "Announcement Message Text",
      "default": "#FFFFFF"
    },
    {
      "variable": "announcement_background_color",
      "label": "Announcement Message Background",
      "default": "#135884"
    }
  ],
  "options": [
    {
      "variable": "announcement_message_text",
      "label": "Announcement text",
      "type": "text",
      "description": "Displays an announcement message on the top of every page"
    },
    {
      "variable": "maintenance_message",
      "label": "Maintenance mode message",
      "type": "text",
      "description": "A message visitors see when your shop is in Maintenance Mode",
      "default": "We're working on our shop right now.<br /><br />Please check back soon."
    },
    {
      "variable": "sidebar_position",
      "label": "Sidebar position",
      "type": "select",
      "options": [
        ["Left", "left"],
        ["Right", "right"]
      ],
      "default": "right",
      "description": "Sets the position of the side navigation on the page"
    },
    {
      "variable": "header_position",
      "label": "Header alignment",
      "type": "select",
      "options": [
        ["Left", "flex-start"],
        ["Center", "center"]
      ],
      "default": "center",
      "description": "Sets the position of your store name or header logo"
    },
    {
      "variable": "featured_header",
      "label": "Featured header",
      "type": "text",
      "description": "Featured products header on the Home page",
      "default": "Featured"
    },
    {
      "variable": "featured_products",
      "label": "Featured products",
      "type": "select",
      "options": "0..100",
      "default": 12,
      "description": "The number of products displayed on the home page"
    },
    {
      "variable": "featured_order",
      "label": "Featured products order",
      "type": "select",
      "options": "product_orders",
      "default": "position",
      "description": "The order of products on the Home page"
    },
    {
      "variable": "all_products_button_text",
      "label": "All products button text",
      "type": "text",
      "description": "Displays underneath featured products and links to your Product Page",
      "default": "All products"
    },
    {
      "variable": "max_products_per_row",
      "label": "Maximum products per row",
      "type": "select",
      "options": "1..4",
      "default": 3,
      "description": "The maximum number of products displayed per row on larger screens"
    },
    {
      "variable": "max_products_per_row_mobile",
      "label": "Maximum products per row (mobile)",
      "type": "select",
      "options": "1..2",
      "default": 2,
      "description": "The number of products displayed per row on smaller screens"
    },
    {
      "variable": "grid_image_style",
      "label": "Product grid image style",
      "type": "select",
      "options": [
        ["Normal", "default"],
        ["Fit to square", "contain"],
        ["Crop to square", "cover"]
      ],
      "default": "cover",
      "description": "Sets the display of images in the product grid"
    },
    {
      "variable": "product_detail_alignment",
      "label": "Product grid details alignment",
      "type": "select",
      "options": [
        ["Left", "left"],
        ["Center", "center"]
      ],
      "default": "left",
      "description": "Alignment for product details under thumbnail images"
    },
    {
      "variable": "product_image_zoom",
      "label": "Enable product image zoom",
      "type": "boolean",
      "default": true,
      "description": "Click to open a larger version of the Product page image"
    },
    {
      "variable": "desktop_product_page_images",
      "label": "Desktop product page images",
      "type": "select",
      "options": [
        ["Thumbnails", "thumbnails"],
        ["Carousel", "carousel"],
        ["Stacked 2 column", "two-column"],
        ["Stacked full width", "stacked"]
      ],
      "default": "carousel",
      "description": "Sets the display of the Product page gallery"
    },
    {
      "variable": "mobile_product_page_images",
      "label": "Mobile product page image carousel",
      "type": "select",
      "options": [
        ["Show thumbnails", "show-thumbnails"],
        ["Hide thumbnails", "hide-thumbnails"]
      ],
      "default": "show-thumbnails",
      "description": "Choose to hide or show thumbnails on the mobile Product page carousel"
    },
    {
      "variable": "products_per_page",
      "label": "Products per page",
      "type": "select",
      "options": "1..100",
      "default": 48,
      "description": "The number of products displayed per page"
    },
    {
      "variable": "money_format",
      "label": "Price display",
      "type": "select",
      "options": [
        ["Currency sign", "sign"],
        ["Currency code with sign", "sign_and_code"]
      ],
      "default": "sign",
      "description": "Sets the display of prices in your shop"
    },
    {
      "variable": "show_search",
      "label": "Show search",
      "type": "boolean",
      "default": true,
      "description": "Displays a search field in the sidebar"
    },
    {
      "variable": "show_newest",
      "label": "Show newest products",
      "type": "boolean",
      "default": false,
      "description": "Displays a list of your newest products in the sidebar"
    },
    {
      "variable": "show_top_selling",
      "label": "Show top selling products",
      "type": "boolean",
      "default": false,
      "description": "Displays a list of your top selling products in the sidebar"
    },
    {
      "variable": "show_sold_out_product_options",
      "label": "Show sold out product options",
      "type": "boolean",
      "default": true,
      "description": "Shows sold out product options in the Product page dropdowns",
      "requires": "inventory"
    },
    {
      "variable": "show_share_buttons",
      "label": "Show Product share buttons",
      "type": "boolean",
      "default": true,
      "description": "Adds Facebook, Twitter, and Pinterest buttons to Product pages"
    },
    {
      "variable": "border_radius",
      "label": "Rounded corners",
      "type": "select",
      "options": [
        ["Off", "0"],
        ["Small", "2px"],
        ["Medium", "4px"],
        ["Large", "8px"]
      ],
      "default": "4px",
      "description": "Controls the rounded corner style on images and other site elements"
    },
    {
      "variable": "instagram_url",
      "label": "Instagram URL",
      "type": "text",
      "description": "Ex: https://instagram.com/bigcartel"
    },
    {
      "variable": "tiktok_url",
      "label": "TikTok URL",
      "type": "text",
      "description": "Ex: https://tiktok.com/@bigcartelofficial"
    },
    {
      "variable": "twitter_url",
      "label": "Twitter URL",
      "type": "text",
      "description": "Ex: https://twitter.com/bigcartel"
    },
    {
      "variable": "facebook_url",
      "label": "Facebook URL",
      "type": "text",
      "description": "Ex: https://facebook.com/bigcartel"
    },
    {
      "variable": "snapchat_url",
      "label": "Snapchat URL",
      "type": "text",
      "description": "Ex: https://snapchat.com/add/bigcartel"
    },
    {
      "variable": "pinterest_url",
      "label": "Pinterest URL",
      "type": "text",
      "description": "Ex: https://pinterest.com/big_cartel"
    },
    {
      "variable": "youtube_url",
      "label": "YouTube URL",
      "type": "text",
      "description": "Ex: https://youtube.com/@BigCartelDotCom"
    },
    {
      "variable": "tumblr_url",
      "label": "Tumblr URL",
      "type": "text",
      "description": "Ex: https://bigcartel.tumblr.com"
    },
    {
      "variable": "bandcamp_url",
      "label": "Bandcamp URL",
      "type": "text",
      "description": "Ex: https://bigcartel.bandcamp.com"
    }
  ],
 "checkout": {
    "optimize_legibility": "optimizeLegibility",
